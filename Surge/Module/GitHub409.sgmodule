#!name=GitHub Status code 409
#!desc=解决github速率限制返回 429 问题
#!system=ios

[MITM]
hostname = %INSERT% raw.githubusercontent.com,gist.githubusercontent.com,github.com

[Header Rewrite]
# 解决github速率限制返回429问题
http-request ^https:\/\/github\.com\/ header-replace Accept-Language en-us
http-request ^https:\/\/(raw|gist)\.githubusercontent\.com\/ header-replace Accept-Language en-us
